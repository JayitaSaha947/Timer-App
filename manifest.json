{
	"manifest_version": 2,
	"name": "Timing Launcher",
	"description": "Quick Launch Timer",
	"version": "1.0.0",
	"icons":{"128": "icon1.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"background": {
		"scripts": ["popup.js"]
	},
	

	"permissions": ["activeTab"]

}